<%= simple_form_for([@experiment, @ppc_ad_copy]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Impressions %>
    <%= f.input :Clicks %>
    <%= f.input :Conversions %>
    <%= f.input :CPC %>
    <%= f.input :title %>
    <%= f.input :line_1 %>
    <%= f.input :line_2 %>
    <%= f.input :display_url %>
    <%= f.input :landing_page_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
